{
  "name": "ng2-pickadate",
  "version": "0.2.2",
  "description": "An Angular2 Datepicker",
  "keywords": [
    "angular",
    "angular2",
    "calendar",
    "datepicker"
  ],
  "homepage": "https://github.com/cschroeter/ng2-pickadate#readme",
  "bugs": {
    "url": "https://github.com/cschroeter/ng2-pickadate/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Christian Schr√∂ter",
      "email": "dev@cschroeter.net"
    },
    {
      "name": "Simon Holzmayer"
    }
  ],
  "main": "ng2-pickadate.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cschroeter/ng2-pickadate.git"
  },
  "scripts": {
    "all": "concurrently \"tsc -w \" \"tsc -p runtime -w \" \"npm run watch-css\" \"npm run lite\" ",
    "build-css": "node-sass --r src -o src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "git clean -f -X",
    "lite": "lite-server",
    "prepublish": "concurrently tsc \"npm run build-css\"",
    "watch": "tsc -w ",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "dependencies": {
    "jquery": "^2.2.4",
    "materialize-css": "^0.97.6",
    "moment": "^2.14.1",
    "pickadate": "^3.5.6"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "concurrently": "^2.0.0",
    "conventional-changelog-cli": "^1.2.0",
    "es6-shim": "^0.35.0",
    "lite-server": "^2.2.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "zone.js": "^0.6.12"
  },
  "peerDependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4"
  }
}

{
  "name": "ng2-pickadate",
  "version": "0.2.1",
  "description": "Angular2 pickadate.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cschroeter/ng2-pickadate.git"
  },
  "bugs": {
    "url": "https://github.com/cschroeter/ng2-pickadate/issues"
  },
  "homepage": "https://github.com/cschroeter/ng2-pickadate#readme",
  "scripts": {
    "prepublish": "concurrently tsc \"npm run build-css\"",
    "build-css": "node-sass --r src -o src",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "all": "concurrently \"tsc -w \" \"tsc -p runtime -w \" \"npm run watch-css\" \"npm run lite\" ",
    "watch": "tsc -w ",
    "lite": "lite-server",
    "clean": "git clean -f -X"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "^2.2.4",
    "materialize-css": "^0.97.6",
    "moment": "^2.14.1",
    "pickadate": "^3.5.6"
  },
  "devDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "@angular2-material/core": "2.0.0-alpha.6",
    "@angular2-material/input": "2.0.0-alpha.6",
    "concurrently": "^2.0.0",
    "es6-shim": "^0.35.0",
    "lite-server": "^2.2.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "zone.js": "^0.6.12"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.4",
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "@angular2-material/core": "2.0.0-alpha.6",
    "@angular2-material/input": "2.0.0-alpha.6"
  },
  "files": [
    "ng2-pickadate.+(d.ts|js|js.map)",
    "dist",
    "src/**/*.+(d.ts|js|js.map|html|css)"
  ]
}
